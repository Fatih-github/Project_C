<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


<!--Template, COPY to use for your own servlet-->
    <servlet>
        <servlet-name>ServletPlan</servlet-name>
        <servlet-class>ServletPlan</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletPlan</servlet-name>
        <url-pattern>/linkPlan</url-pattern>
    </servlet-mapping>

<!--Fires on login with google-->
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>ServletLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <!--Fires when Reservations button is clicked-->
    <servlet>
        <servlet-name>ServletReservations</servlet-name>
        <servlet-class>ServletReservations</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletReservations</servlet-name>
        <url-pattern>/linkReservations</url-pattern>
    </servlet-mapping>

    <!--Fires when Invitations button is clicked-->
    <servlet>
        <servlet-name>ServletInvitations</servlet-name>
        <servlet-class>ServletInvitations</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletInvitations</servlet-name>
        <url-pattern>/linkInvitations</url-pattern>
    </servlet-mapping>

</web-app>