<!--Fires on login with google-->
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>ServletLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <!--Fires when Reservations button is clicked-->
    <servlet>
        <servlet-name>ServletReservations</servlet-name>
        <servlet-class>ServletReservations</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletReservations</servlet-name>
        <url-pattern>/linkReservations</url-pattern>
    </servlet-mapping>

    <!--Fires when Invitations button is clicked-->
    <servlet>
        <servlet-name>ServletInvitations</servlet-name>
        <servlet-class>ServletInvitations</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletInvitations</servlet-name>
        <url-pattern>/linkInvitations</url-pattern>
    </servlet-mapping>


    <!--Fires when Home logo is clicked-->
    <servlet>
        <servlet-name>ServletHome</servlet-name>
        <servlet-class>ServletHome</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletHome</servlet-name>
        <url-pattern>/linkHome</url-pattern>
    </servlet-mapping>


    <!--Fires when delete reservation button is clicked-->
    <servlet>
        <servlet-name>ServletDeleteReservations</servlet-name>
        <servlet-class>ServletDeleteReservations</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletDeleteReservations</servlet-name>
        <url-pattern>/linkDeleteReservations</url-pattern>
    </servlet-mapping>

    <!--Fires when update reservation button is clicked-->
    <servlet>
        <servlet-name>ServletUpdateReservations</servlet-name>
        <servlet-class>ServletUpdateReservations</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletUpdateReservations</servlet-name>
        <url-pattern>/linkUpdateReservations</url-pattern>
    </servlet-mapping>


